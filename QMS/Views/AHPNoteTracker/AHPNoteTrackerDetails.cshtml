@using QMS.Core.Models

@model AHPNoteViewModel

@{
    ViewData["Title"] = "AHP Note Tracker";
    int currentMonth = DateTime.Now.Month;
    int currentQ = 0;
    if (currentMonth >= 4 && currentMonth <= 6) { currentQ = 1; }
    else if (currentMonth >= 7 && currentMonth <= 9) { currentQ = 2; }
    else if (currentMonth >= 10 && currentMonth <= 12) { currentQ = 3; }
    else { currentQ = 4; }
    bool showQ1 = currentQ >= 1;
    bool showQ2 = currentQ >= 2;
    bool showQ3 = currentQ >= 3;
    bool showQ4 = currentQ == 4;
    int startYY = Model.FinancialYear / 100; // 2425 / 100 = 24
    int endYY = Model.FinancialYear % 100;   // 2425 % 100 = 25
    string currFY = $"FY{startYY:D2}-{endYY:D2}";
    string prevFY = $"FY{(startYY - 1):D2}-{(endYY - 1):D2}";
    int countQ = 0;
    if (showQ1) countQ++;
    if (showQ2) countQ++;
    if (showQ3) countQ++;
    if (showQ4) countQ++;
    int colspanWithYTD = countQ + 1;
}

<!-- ===== Header Card ===== -->
<div class="card">
    <div class="card-body d-flex justify-content-between align-items-center">
        <div>
            <b style="font-size: large; color: #4682B4;">AHP Note Tracker</b>
        </div>
        <div>
            <button id="saveButton" type="button" class="btn btn-outline-success mr-2" onclick="insertUpdateAHPNote()" style="width:120px;font-size:15px">
                <i class="fas fa-save mr-2 fa-1x"></i> SAVE
            </button>
            <button id="backButton" class="btn btn-outline-primary"
                    onclick="location.href='@Url.Action("AHPNoteTracker","AHPNoteTracker")'"
                    style="width:120px;font-size:15px">
                <i class="fas fa-arrow-left"></i> BACK
            </button>
        </div>
    </div>
</div>

