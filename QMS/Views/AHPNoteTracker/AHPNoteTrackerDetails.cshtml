@section Scripts {
    <!-- ===== Required JS for tabs (Bootstrap 5 bundle) ===== -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>

    <script>
        function insertUpdateAHPNote() {
            Blockloadershow();
            $("#Id").val(@Model.Id);
            $("#FinancialYear").val(@Model.FinancialYear);
            const form = $('#ahpNote-form')[0];
            const formData = new FormData(form);
            $('.see-matrix [contenteditable="true"]').each(function() {
                const dataName = $(this).data('name');
                const content = $(this).text().trim();

                if (dataName) {
                    formData.append(dataName, content);
                }
            });

            $.ajax({
                url: $('#ahpNote-form').attr('action'),
                type: 'POST',
                data: formData,
                processData: false,
                contentType: false,
                success: function (response) {
                    Blockloaderhide();
                    if (!response.success) {
                        var errorMessg = "";
                        if (response.errors != undefined) {
                            for (var error of response.errors) {
                                errorMessg += error + "\n";
                            }
                            if (errorMessg != "") {
                                showDangerAlert(errorMessg);
                            }
                        }
                        else {
                            showDangerAlert(response.message);
                        }
                        return false;
                    }
                    else {
                        if ($("#Id").val() > 0) {
                            showSuccessAlert("Updated Successfully.");
                        }
                        else {
                            showSuccessAlert("Saved Successfully.");
                        }
                        setTimeout(function () {
                            window.open('@Url.Action("AHPNotetracker", "AHPNoteTracker")', '_self');
                        }, 2500);
                    }
                },
                error: function (xhr, status, error) {
                    Blockloaderhide();
                    showDangerAlert('Error saving data: ' + error);
                }
            });
        }
    </script>
}
