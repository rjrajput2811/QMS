                                                    <th style="width:3%; text-align:center;">S.N.</th>
                                                    <th style="width:10%; text-align:left;">Projects</th>
                                                    <th class="c-target">Target</th>
                                                    <th class="c-q">FY23-24<br>Q1</th>
                                                    <th class="c-q">FY23-24<br>Q2</th>
                                                    <th class="c-q">FY23-24<br>Q3</th>
                                                    <th class="c-q">FY23-24<br>Q4</th>
