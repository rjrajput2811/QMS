        <!-- Top-level tabs -->
        <div class="nav nav-tabs" id="cib-tabs" role="tablist">
            <a class="nav-link active" id="cib-lighting-tab" data-bs-toggle="tab" href="#cib-lighting" role="tab">CIB Lighting</a>
            <a class="nav-link" id="cib-seating-tab" data-bs-toggle="tab" href="#cib-seating" role="tab">CIB Seating</a>
        </div>

        <div class="tab-content" id="cib-tabContent">
            <!-- ========== CIB LIGHTING ========== -->
            <div class="tab-pane fade show active" id="cib-lighting" role="tabpanel">
                <div class="mt-3">
                    <!-- Inner tabs (Lighting only) -->
                    <div class="nav nav-tabs" id="cibLightingTabs" role="tablist">
                        <a class="nav-link active" id="see-matrix-tab" data-bs-toggle="tab" href="#see-matrix" role="tab">SEE Matrix</a>
                        <a class="nav-link" id="six-sigma-tab" data-bs-toggle="tab" href="#six-sigma" role="tab">Six Sigma Projects</a>
                        <a class="nav-link" id="csat-tab" data-bs-toggle="tab" href="#csat" role="tab">CSAT</a>
                        <a class="nav-link" id="spm-score-tab" data-bs-toggle="tab" href="#spm-score" role="tab">SPM Score</a>
                        <a class="nav-link" id="otif-tab" data-bs-toggle="tab" href="#otif" role="tab">On Time in Full Delivery</a>
                        <a class="nav-link" id="service-tab" data-bs-toggle="tab" href="#service" role="tab">Service Complaint Closure</a>
                        <a class="nav-link" id="cso-tab" data-bs-toggle="tab" href="#cso" role="tab">CSO</a>
                        <a class="nav-link" id="cost-tab" data-bs-toggle="tab" href="#cost" role="tab">Cost Savings / Target</a>
                        <a class="nav-link" id="oql-tab" data-bs-toggle="tab" href="#oql" role="tab">Outgoing Quality Level</a>
                    </div>

                    <div class="tab-content mt-3" id="cibLightingTabContent">
