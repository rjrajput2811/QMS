<script type="text/javascript">
    let searchTimeout;
    var modelDetails = @Html.Raw(Json.Serialize(Model.Details ?? new List<TemperatureRiseDetailModal>()));

    $(document).ready(function () {
        console.log("Document ready - initializing search");

        const inputElement = $("#prodCode_Search");
        if (inputElement.length) {
            console.log("prodCode_Search input found");

            // Attach the input event handler
            inputElement.on('input', function (event) {
                handleProdCodeSearch(event);
            });
        } else {
            console.error("prodCode_Search input NOT found");
        }

        if (modelDetails && modelDetails.length > 0) {
            console.log("Found " + modelDetails.length + " detail records");

            // Populate each row
            modelDetails.forEach(function(detail, index) {
                var rowIndex = index + 1; // Rows are 1-based

                // Set all thermal reading values
                $('input[name="ThermalReadings[' + rowIndex + '].TimeHrs"]').val(detail.timeHrs || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T1"]').val(detail.t1 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T2"]').val(detail.t2 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T3"]').val(detail.t3 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T4"]').val(detail.t4 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T5"]').val(detail.t5 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T6"]').val(detail.t6 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T7"]').val(detail.t7 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T8"]').val(detail.t8 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T9"]').val(detail.t9 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T10"]').val(detail.t10 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T11"]').val(detail.t11 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T12"]').val(detail.t12 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T13"]').val(detail.t13 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T14"]').val(detail.t14 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T15"]').val(detail.t15 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].T16"]').val(detail.t16 || '');
                $('input[name="ThermalReadings[' + rowIndex + '].VIN"]').val(detail.vin || '');
                $('input[name="ThermalReadings[' + rowIndex + '].IIN"]').val(detail.iin || '');
                $('input[name="ThermalReadings[' + rowIndex + '].PIN"]').val(detail.pin || '');
                $('input[name="ThermalReadings[' + rowIndex + '].TJ"]').val(detail.tj || '');
            });

            console.log("Thermal readings loaded successfully");
        } else {
            console.log("No thermal readings to load (new record)");
        }

    });

    function handleProdCodeSearch(event) {
        console.log("handleProdCodeSearch called");

        const input = event.target;
        if (!input) {
            console.error("Input element not found");
            return;
        }

        let searchQuery = input.value.trim();
        console.log("Search query:", searchQuery);

        // Clear suggestions if less than 4 chars
        if (searchQuery.length < 4) {
            clearTimeout(searchTimeout);
            removeExistingDropdown(input);
            return;
        }

        // Take first 4 characters only
        const searchTerms = searchQuery.substring(0, 4);
        console.log("Search terms (first 4 chars):", searchTerms);

        // Debounce API call
        clearTimeout(searchTimeout);
        searchTimeout = setTimeout(() => {
            fetchProdCodeRelatedData(searchTerms, input);
        }, 300);
    }

    function fetchProdCodeRelatedData(productCatNo, inputField) {
        console.log("Fetching data for:", productCatNo);

        $.ajax({
            url: '/Vendor/GetCodeSearch',
            type: 'GET',
            data: { search: productCatNo },
            dataType: 'json',
            success: function (data) {
                console.log("Data received:", data);

                if (data && data.length > 0) {
                    displayProdCodeSuggestions(data, inputField);
                } else {
                    console.log("No data found");
                    showDangerAlert('No data found for the entered Product Cat No.');
                    removeExistingDropdown(inputField);
                }
            },
            error: function (xhr, status, error) {
                console.error('AJAX Error - Status:', status);
                console.error('Error:', error);
                console.error('Response:', xhr.responseText);
                showDangerAlert('Error fetching data: ' + error);
            }
        });
    }

    // Function to display suggestions in a dropdown
    function displayProdCodeSuggestions(data, inputField) {
        console.log("Displaying suggestions, count:", data.length);

        // Remove any existing dropdown
        removeExistingDropdown(inputField);

        // Ensure parent can anchor absolute dropdown
        const parent = inputField.parentElement;
        if (getComputedStyle(parent).position === "static") {
            parent.style.position = "relative";
        }

        const dropdown = document.createElement('div');
        dropdown.className = 'suggestion-dropdown';
        dropdown.style.position = 'absolute';
        dropdown.style.top = (inputField.offsetTop + inputField.offsetHeight) + 'px';
        dropdown.style.left = inputField.offsetLeft + 'px';
        dropdown.style.backgroundColor = 'white';
        dropdown.style.border = '1px solid #ccc';
        dropdown.style.zIndex = '1000';
        dropdown.style.overflowY = 'auto';
        dropdown.style.maxHeight = '300px';
        dropdown.style.width = inputField.offsetWidth + 'px';
        dropdown.style.boxShadow = '0px 4px 6px rgba(0, 0, 0, 0.1)';

        data.forEach(item => {
            const code = (item.oldPart_No || "").toString().trim();
            const desc = (item.description || item.prodDesc || item.prod_Desc || "").toString().trim();

            const option = document.createElement('div');
            option.className = 'suggestion-item';
            option.textContent = desc ? `${code} | ${desc}` : code;
            option.style.padding = '8px';
            option.style.cursor = 'pointer';

            option.addEventListener('mouseover', () => {
                option.style.backgroundColor = '#4682B4';
                option.style.color = '#fff';
            });

            option.addEventListener('mouseout', () => {
                option.style.backgroundColor = '';
                option.style.color = '';
            });

            option.addEventListener('click', () => {
                console.log("Selected code:", code, "desc:", desc);

                const productCatRefInput = document.getElementById('ProductCatRef');
                if (productCatRefInput) {
                    productCatRefInput.value = code ?? '';
                    console.log("Set ProductCatRef to:", code);
                }

                const productDescInput = document.getElementById('ProductDescription');
                if (productDescInput) {
                    productDescInput.value = desc ?? '';
                    console.log("Set ProductDescription to:", desc);
                }

                const searchBox = document.getElementById('prodCode_Search');
                if (searchBox) {
                    searchBox.value = code;
                    console.log("Set search box to:", code);
                }

                dropdown.remove();
            });

            dropdown.appendChild(option);
        });

        parent.appendChild(dropdown);
        console.log("Dropdown appended to parent");
    }

    // Function to remove any existing dropdowns
    function removeExistingDropdown(inputField) {
        const existingDropdown = document.querySelector('.suggestion-dropdown');
        if (existingDropdown) {
            existingDropdown.remove();
            console.log("Existing dropdown removed");
        }
    }

    // Remove dropdown on outside click
    document.addEventListener('click', function (e) {
        if (!e.target.closest('.suggestion-dropdown') && !e.target.closest('#prodCode_Search')) {
            removeExistingDropdown();
        }
    });

    function SaveTemperatureTest() {
        console.log("SaveTemperatureTest called");

        var idVal = $('#Id').val();
        idVal = idVal ? parseInt(idVal) : 0;

        // Collect thermal readings
        var thermalRows = [];
        var thermalMap = {};

        $('input[name^="ThermalReadings["]').each(function () {
            var name = $(this).attr('name');
            var match = name.match(/ThermalReadings\[(\d+)\]\.(.+)/);
            if (!match) return;

            var index = match[1];
            var field = match[2];
            var value = $(this).val() || '';

            if (!thermalMap[index]) thermalMap[index] = {};
            thermalMap[index][field] = value;
        });

        Object.keys(thermalMap).sort(function (a, b) {
            return a - b;
        }).forEach(function (i) {
            var d = thermalMap[i];

            var row =
                (d.TimeHrs || '') + '^' +
                (d.T1 || '') + '^' +
                (d.T2 || '') + '^' +
                (d.T3 || '') + '^' +
                (d.T4 || '') + '^' +
                (d.T5 || '') + '^' +
                (d.T6 || '') + '^' +
                (d.T7 || '') + '^' +
                (d.T8 || '') + '^' +
                (d.T9 || '') + '^' +
                (d.T10 || '') + '^' +
                (d.T11 || '') + '^' +
                (d.T12 || '') + '^' +
                (d.T13 || '') + '^' +
                (d.T14 || '') + '^' +
                (d.T15 || '') + '^' +
                (d.T16 || '') + '^' +
                (d.VIN || '') + '^' +
                (d.IIN || '') + '^' +
                (d.PIN || '') + '^' +
                (d.TJ || '');

            thermalRows.push(row);
        });

        var model = {
            Id: idVal,
            ProductCatRef: $('#ProductCatRef').val(),
            ProductDescription: $('#ProductDescription').val(),
            ReportNo: $('#ReportNo').val(),
            ReportDate: $('#ReportDate').val(),
            PKD: $('#PKD').val(),
            TestingLocation: $('#TestingLocation').val(),
            BatchCode: $('#BatchCode').val(),
            HeatSinkMaterial: $('#HeatSinkMaterial').val(),
            HeatSinkWeight: $('#HeatSinkWeight').val(),
            LensDetails: $('#LensDetails').val(),
            ThermalPasteDetails: $('#ThermalPasteDetails').val(),
            NominalOperatingVoltage: $('#NominalOperatingVoltage').val(),

            DriverUsed: $('#DriverUsed').val(),
            LedUsed: $('#LedUsed').val(),
            NoOfDrivers: $('#NoOfDrivers').val(),
            DriverOutputVoltage: $('#DriverOutputVoltage').val(),
            DriverOutputCurrent: $('#DriverOutputCurrent').val(),
            NoOfLedsInSeries: $('#NoOfLedsInSeries').val(),
            NoOfLedsInParallel: $('#NoOfLedsInParallel').val(),
            NoOfLeds: $('#NoOfLeds').val(),
            DriverAllowableTc: $('#DriverAllowableTc').val(),
            LedRjTH: $('#LedRjTH').val(),

            DriverAllowableTa: $('#DriverAllowableTa').val(),
            LedVf: $('#LedVf').val(),
            PcbMaterialMake: $('#PcbMaterialMake').val(),
            LedIf: $('#LedIf').val(),
            PcbSizeQty: $('#PcbSizeQty').val(),
            LedWdc: $('#LedWdc').val(),

            ProbeT1_Desc: $('#ProbeT1_Desc').val(),
            ProbeT2_Desc: $('#ProbeT2_Desc').val(),
            ProbeT3_Desc: $('#ProbeT3_Desc').val(),
            ProbeT4_Desc: $('#ProbeT4_Desc').val(),
            ProbeT5_Desc: $('#ProbeT5_Desc').val(),
            ProbeT6_Desc: $('#ProbeT6_Desc').val(),
            ProbeT7_Desc: $('#ProbeT7_Desc').val(),
            ProbeT8_Desc: $('#ProbeT8_Desc').val(),
            ProbeT9_Desc: $('#ProbeT9_Desc').val(),
            ProbeT10_Desc: $('#ProbeT10_Desc').val(),
            ProbeT11_Desc: $('#ProbeT11_Desc').val(),
            ProbeT12_Desc: $('#ProbeT12_Desc').val(),
            ProbeT13_Desc: $('#ProbeT13_Desc').val(),
            ProbeT14_Desc: $('#ProbeT14_Desc').val(),
            ProbeT15_Desc: $('#ProbeT15_Desc').val(),
            ProbeT16_Desc: $('#ProbeT16_Desc').val(),

            Conclusion_MaxRecordedTJ: $('#Conclusion_MaxRecordedTJ').val(),
            Conclusion_AllowableTJ: $('#Conclusion_AllowableTJ').val(),
            Conclusion_MaxRecordedDriverTc: $('#Conclusion_MaxRecordedDriverTc').val(),
            Conclusion_AllowableDriverTc: $('#Conclusion_AllowableDriverTc').val(),
            Conclusion_MaxRecordedLensTemp: $('#Conclusion_MaxRecordedLensTemp').val(),
            Conclusion_OverThermalCutoff: $('#Conclusion_OverThermalCutoff').val(),
            Conclusion_Result: $('#Conclusion_Result').val(),

            ConductedBy: $('#ConductedBy').val(),
            WitnessBy: $('#WitnessBy').val(),
            ApprovedBy: $('#ApprovedBy').val(),
            ConductedBySecnd: $('#ConductedBySecnd').val(),
            WitnessBySecnd: $('#WitnessBySecnd').val(),
            ApprovedBySecnd: $('#ApprovedBySecnd').val(),

            // Thermal readings as delimited string
            ThermalReadingsData: thermalRows.join('~'),

            // Maximum values
            MaxValues_T1: $('input[name="MaxValues.T1"]').val(),
            MaxValues_T2: $('input[name="MaxValues.T2"]').val(),
            MaxValues_T3: $('input[name="MaxValues.T3"]').val(),
            MaxValues_T4: $('input[name="MaxValues.T4"]').val(),
            MaxValues_T5: $('input[name="MaxValues.T5"]').val(),
            MaxValues_T6: $('input[name="MaxValues.T6"]').val(),
            MaxValues_T7: $('input[name="MaxValues.T7"]').val(),
            MaxValues_T8: $('input[name="MaxValues.T8"]').val(),
            MaxValues_T9: $('input[name="MaxValues.T9"]').val(),
            MaxValues_T10: $('input[name="MaxValues.T10"]').val(),
            MaxValues_T11: $('input[name="MaxValues.T11"]').val(),
            MaxValues_T12: $('input[name="MaxValues.T12"]').val(),
            MaxValues_T13: $('input[name="MaxValues.T13"]').val(),
            MaxValues_T14: $('input[name="MaxValues.T14"]').val(),
            MaxValues_T15: $('input[name="MaxValues.T15"]').val(),
            MaxValues_T16: $('input[name="MaxValues.T16"]').val(),
            MaxValues_VIN: $('input[name="MaxValues.VIN"]').val(),
            MaxValues_IIN: $('input[name="MaxValues.IIN"]').val(),
            MaxValues_PIN: $('input[name="MaxValues.PIN"]').val(),
            MaxValues_TJ: $('input[name="MaxValues.TJ"]').val()
        };

        console.log("Model data:", model);

        $.ajax({
            url: '/ProductValidation/InsertUpdateTemperatureRiseDetails',
            type: 'POST',
            data: model,
            success: function (res) {
                console.log("Save response:", res);
                if (model.Id > 0) {
                    showSuccessAlert("Updated Successfully.");
                } else {
                    showSuccessAlert("Saved Successfully.");
                }
            },
            error: function (xhr, status, error) {
                console.error("Save error:", error);
                console.error("Response:", xhr.responseText);
                if (typeof Blockloaderhide === 'function') {
                    Blockloaderhide();
                }
                showDangerAlert('Error saving data: ' + error);
            }
        });
    }

    // Function called by Save button - make sure this exists
    function insertTemperatureTest() {
        console.log("insertTemperatureTest called");
        saveElectricalPerformance();
    }
</script>
 

