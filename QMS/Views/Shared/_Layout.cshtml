    <script>
        $(document).ready(function () {
            var currentUrl = window.location.href.toLowerCase();

            // Highlight direct (non-submenu) links
            $('ul.nav-sidebar a.nav-link').filter(function () {
            return this.href && this.href.toLowerCase() === currentUrl;
            }).addClass('active');

            // Highlight and open submenu items
            $('ul.nav-group-sub a.nav-link').filter(function () {
            return this.href && this.href.toLowerCase() === currentUrl;
            }).addClass('active') // highlight link
            .closest('.nav-item-submenu') // find parent submenu
            .addClass('nav-item-expanded nav-item-open'); // open submenu
            });
            document.addEventListener("DOMContentLoaded", function () {
            const hash = window.location.hash.replace('#', '');

            // Define mapping for valid top-level menu IDs
            const allowedMenus = ["Supply_Chain", "Service", "Trackers"];

            if (allowedMenus.includes(hash)) {
            // Hide all top-level menu items
            document.querySelectorAll(".nav-sidebar > .nav-item").forEach(item => {
            item.style.display = "none";
            });

            // Show only the matched one
            const menuLink = document.getElementById(hash);
            const menuItem = menuLink?.closest("li.nav-item");
            const submenu = menuLink?.nextElementSibling;

            if (menuItem) {
            menuItem.style.display = "block";
            menuItem.classList.add("nav-item-open");

            if (submenu && submenu.classList.contains("nav-group-sub")) {
            submenu.classList.add("d-block");
            }
            }
            } else {
            // Show all menus if no hash or unknown hash
            document.querySelectorAll(".nav-sidebar > .nav-item").forEach(item => {
            item.style.display = "block";
            });
            }


        });

            
